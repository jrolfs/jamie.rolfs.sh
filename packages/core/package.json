{
  "name": "@jrolfs/core",
  "version": "0.0.1",
  "main": "dist/jrolfs-core.cjs.js",
  "module": "dist/jrolfs-core.esm.js",
  "exports": {
    ".": {
      "require": "./dist/jrolfs-core.cjs.js",
      "import": "./dist/jrolfs-core.esm.js",
      "default": "./dist/jrolfs-core.cjs.js"
    }
  },
  "type": "module",
  "repository": "git+ssh://github.com/jrolfs/jamie.rolfs.sh/tree/master/packages/core",
  "author": "Jamie Rolfs <jamie@rolfs.sh>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "run-p 'build:*'",
    "build:source": "",
    "build:types": "tsc",
    "commit": "hover-scripts commit",
    "format": "hover-scripts format",
    "lint": "hover-scripts lint",
    "pre-commit": "hover-scripts pre-commit --test-command 'hover-scripts test --findRelatedTests --passWithNoTests'",
    "start": "run-p 'start:*'",
    "start:source": "",
    "start:types": "tsc -b -w --preserveWatchOutput",
    "test": "hover-scripts test --no-watch",
    "test:update": "hover-scripts test --updateSnapshot",
    "test:watch": "hover-scripts test"
  },
  "devDependencies": {
    "@hover/javascript": "7.0.0-beta.6",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.3"
  }
}
