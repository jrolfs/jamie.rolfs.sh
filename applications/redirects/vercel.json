{
  "redirects": [
    {
      "source": "/twitter",
      "destination": "https://twitter.com/jrolfs",
      "permanent": true
    },
    {
      "source": "/bluesky",
      "destination": "https://bluesky.com/jrolfs",
      "permanent": true
    },
    {
      "source": "/mastodon",
      "destination": "https://mastodon.social/@jrolfs",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "destination": "https://github.com/jrolfs/$1"
    }
  ]
}
